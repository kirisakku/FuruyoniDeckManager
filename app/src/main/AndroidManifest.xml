        <activity android:name="com.example.furuyonideckmanager2.SelectDeckActivity" />
        <activity android:name="com.example.furuyonideckmanager2.RandomChooseActivity" />
        <activity
            android:name="com.example.furuyonideckmanager2.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>